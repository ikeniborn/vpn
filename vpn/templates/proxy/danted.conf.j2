# Dante SOCKS5 Configuration
# Generated for {{ server_name }}

# Log output
logoutput: /var/log/danted.log

# Internal interface
internal: 0.0.0.0 port = 1080

# External interface
external: eth0

{% if auth_enabled %}
# Authentication method
socksmethod: username
clientmethod: none

# Authentication file
user.privileged: root
user.unprivileged: nobody
{% else %}
# No authentication
socksmethod: none
clientmethod: none
{% endif %}

# Client access rules
client pass {
    from: 0.0.0.0/0 to: 0.0.0.0/0
    log: connect disconnect error
}

# SOCKS rules
socks pass {
    from: 0.0.0.0/0 to: 0.0.0.0/0
    command: bind connect udpassociate
    log: connect disconnect error
    {% if auth_enabled %}
    socksmethod: username
    {% endif %}
}