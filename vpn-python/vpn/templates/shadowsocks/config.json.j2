{
  "server": "0.0.0.0",
  "server_port": {{ server.port }},
  "password": "{{ password }}",
  "method": "{{ cipher }}",
  "timeout": {{ timeout }},
  "fast_open": true,
  "reuse_port": true,
  "no_delay": true,
  "mode": "tcp_and_udp",
  "nameserver": {{ dns | tojson }},
  {% if access_keys %}
  "port_password": {
    {% for key in access_keys %}
    "{{ key.port }}": "{{ key.password }}"{% if not loop.last %},{% endif %}
    {% endfor %}
  },
  {% endif %}
  "plugin": "v2ray-plugin",
  "plugin_opts": "server;path=/ss"
}